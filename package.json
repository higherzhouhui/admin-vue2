    "@vue/cli-service": "3.11.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "autoprefixer": "^9.6.4",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.3",
    "babel-jest": "24.9.0",
    "chalk": "2.4.2",
    "connect": "3.7.0",
    "decimal.js": "^10.4.3",
    "eslint": "6.5.1",
    "eslint-plugin-vue": "5.2.3",
