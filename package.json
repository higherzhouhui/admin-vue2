    "element-ui": "^2.12.0",
    "file-saver": "^2.0.2",
    "js-cookie": "2.2.1",
    "jszip": "3.2.2",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.41",
    "node-ipc": "^9.0.0",
    "normalize.css": "8.0.1",
    "nprogress": "0.2.0",
    "path-to-regexp": "3.1.0",
    "qs": "^6.10.3",
    "sass": "^1.54.8",
    "vue": "2.6.10",
    "vue-count-to": "^1.0.13",
    "vue-router": "3.1.3",
